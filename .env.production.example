# Frontend Production Environment Variables
# Copy this to .env.production and fill in your deployed URLs

# FHIR Server URL (from Render deployment)
VITE_FHIR_SERVER_URL=https://your-fhir-server.onrender.com

# Backend API URL (from Render deployment)
VITE_BACKEND_URL=https://your-backend.onrender.com

# Supabase Configuration (from Supabase dashboard)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Card Backend URL (if deploying card system)
VITE_CARD_BACKEND_URL=http://localhost:3001
